{
  "expo": {
    "name": "poker-app",
    "slug": "poker-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons8-cards-64.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pokerstats.poker"
    },
    "android": {
      "package": "com.pokerstats.poker"
    },
    "description": "An app to track you poker playing statistics."
  }
}
